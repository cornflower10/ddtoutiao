    android:shape="rectangle" >

    <solid android:color="@color/transparent" />

    <corners
        android:radius="@dimen/DIMEN_6DP"/>

    <padding
        android:bottom="0.5dp"
        android:left="0.5dp"
        android:right="0.5dp"
