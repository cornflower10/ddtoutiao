        <item name="android:activityOpenEnterAnimation">@anim/common_open_enter</item>
        <item name="android:activityOpenExitAnimation">@anim/common_open_exit</item>
        <item name="android:activityCloseEnterAnimation">@anim/common_close_enter</item>
        <item name="android:activityCloseExitAnimation">@anim/common_close_exit</item>
